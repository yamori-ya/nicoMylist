{
  "name": "nyMylist",
  "version": "0.9.0",
  "description": "ニコニコ動画のマイリスト拡張",
  "manifest_version": 2,
  
  "browser_action": {
    "default_title": "nicoMylist",
      "default_icon": {
      },
      "default_popup": "html/pop.html"
  },
  
  "content_scripts": [ {
      "js": [ "js/jquery-3.3.1.min.js", "js/SheetApi.js", "js/function.js", "js/content.js" ],
      "css": [ "css/content.css" ],
      "matches": [ 
        "http://*.nicovideo.jp/watch/*",
        "https://*.nicovideo.jp/watch/*",
        "https://www.youtube.com/watch*"
      ]
   } ],
   
  "background": {
    "scripts": [ "js/function.js", "js/background.js", "js/SheetApi.js" ],
    "persistent": false
  },
   
  "permissions": [
    "identity",
    "storage",
    "unlimitedStorage",
    "tabs"
  ],
  "options_ui": {
    "page": "html/option.html",
    "open_in_tab": true
  },
  
  "oauth2": {
    "client_id": "645932863093-aboaes2a3r2mhtd3ga2s96tj692gqrbu.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/spreadsheets"]
  },
  "key": ""
}
